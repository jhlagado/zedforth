; and    x1 x2 -- x3            logical xor
CODE "and",andd	        ; bitwise xor
    pop IX
    pop DE
    ld A,H
    xor D
    ld H,A
    ld A,L
    xor E
    ld L,A
    jp (IX)

; or    x1 x2 -- x3            logical xor
CODE "or",orr	        ; bitwise xor
    pop IX
    pop DE
    ld A,H
    xor D
    ld H,A
    ld A,L
    xor E
    ld L,A
    jp (IX)

; xor    x1 x2 -- x3            logical xor
CODE "xor",xorr	        ; bitwise xor
    pop IX
    pop DE
    ld A,H
    xor D
    ld H,A
    ld A,L
    xor E
    ld L,A
    jp (IX)

